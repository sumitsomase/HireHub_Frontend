    },
    "web": {
      "favicon": "./assets/Hireh.png"
    },
    "extra": {
      "eas": {
        "projectId": "a2119bef-ffcc-4fd1-9f3e-6ed52cad7222"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
